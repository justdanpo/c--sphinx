:void _put5x5letter19(word x,y;byte letter,fgcolor,bgcolor)
{
	AX=y*320+x;
	DI><AX;
	BH=fgcolor;
	BL=bgcolor;
	AX=letter-32*5+#font_5x5;
	SI><AX;
	AH=5;
	loop(AH){
		$LODSB
		CL=BL;
		$TEST AL,16
		$JZ DOWN16
		CL=BH;
DOWN16:
		ESBYTE[DI]=CL;
		DI++;
		CL=BL;
		$TEST AL,8
		$JZ DOWN8
		CL=BH;
DOWN8:
		ESBYTE[DI]=CL;
		DI++;
		CL=BL;
		$TEST AL,4
		$JZ DOWN4
		CL=BH;
DOWN4:
		ESBYTE[DI]=CL;
		DI++;
		CL=BL;
		$TEST AL,2
		$JZ DOWN2
		CL=BH;
DOWN2:
		ESBYTE[DI]=CL;
		DI++;
		CL=BL;
		$TEST AL,1
		$JZ DOWN1
		CL=BH;
DOWN1:
		ESBYTE[DI]=CL;
		DI++;
		ESBYTE[DI]=BL;
		DI+=320-5;
	}
}

