: void putresized19 (word x,y,newxsize,newysize,imageloc)
word xsize,ysize;
{
ES = 0xA000;
BX = imageloc;
xsize = DSWORD[BX];
ysize = DSWORD[BX+2];
imageloc += 4;
AX = y*320 + x;
SI = AX;
y = 0;
do {
    CX = 1;
    DI = SI;
    do {
        CX--;
        AX = CX*xsize;
        $ DIV newxsize
        BX = AX;
        AX = y*ysize;
        $ DIV newysize
        AX = AX*xsize + BX + imageloc;
        BX = AX;
        ESBYTE[DI] = DSBYTE[BX];
        DI++;
        CX++;
        CX++;
	} while( CX < newxsize );
    SI += 320;
    y++;
    } while( y < newysize );
}


