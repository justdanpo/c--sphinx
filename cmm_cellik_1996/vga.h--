: void UNBLANKVGA ()
{
DX = 0x3C6;
AL = 0xFF;
$ OUT DX,AL
}


: void BLANKVGA ()
{
DX = 0x3C6;
AL = 0;
$ OUT DX,AL
}


: void XLINE19 ()   /* AX = xloc, BX = yloc, CX = length, DL = colour */
{
DI = AX;
BX >< DX;
AX = 320 * DX;
DI += AX;
ES = 0xA000;
AL = BL;
$ REPZ
$ STOSB
}


