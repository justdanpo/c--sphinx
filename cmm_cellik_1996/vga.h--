: void CLEARSCREEN19 ()
{DI = 0;
CX = 64000/2;
ES = 0xA000;
AX = 0;
$ REPZ
$ STOSW
}


: void _CLEARSCREEN19 ()
{DI = 0;
CX = 64000/2;
AX = 0;
$ REPZ
$ STOSW
}


: void FILLSCREEN19 ()   // AL = colour to fill screen
{DI = 0;
CX = 0xA000;
ES = CX;
CX = 64000/2;
AH = AL;
$ REPZ
$ STOSW
}


: void _FILLSCREEN19 ()   // AL = colour to fill screen
{DI = 0;
CX = 64000/2;
AH = AL;
$ REPZ
$ STOSW
}


: void putpixel19 (word x,y; byte color)
{
ES = 0xA000;
AX = 320*y + x;
BX = AX;
ESBYTE[BX] = color;
}


: void _putpixel19 (word x,y; byte color)
{
AX = 320*y + x;
BX = AX;
